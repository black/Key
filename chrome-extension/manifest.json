{
    "manifest_version": 2,
    "name": "PasswordKey",
    "description": "Demo for connecting chrome exteion to localserver",
    "version": "1.0",
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>"
    ]
}